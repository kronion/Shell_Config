# Basic directory operations
alias ~='cd ~'
alias -- -='cd -'

# Config shortcuts
alias rc='vim ~/.zshrc'
alias rcu='. ~/.zshrc'
alias vimrc='vim ~/.vimrc'

# Miscellaneous
alias c='clear'
alias rm='rm -i'
alias VIM='vim'

# Open Finder at current directory
alias o='open .'

# Virtualenv
alias activate='source bin/activate'
